#breadcrumb
  =link_to "Admin", admin_path
  =">"
  =link_to "Users", admin_users_path
%h1 Users
%table
  %tr
    %th User
    %th Email
  -for user in @users
    %tr
      -if user.admin
        %td.red= link_to user, admin_user_path(user)
      -else
        %td= link_to user, admin_user_path(user)
      %td= user.email
